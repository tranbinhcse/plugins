{
    "name": "tinoapp/plugins",
    "description": "Core stuff for Tino plugin support.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tran Binh",
            "email": "binh@tino.org",
            "homepage": "https://tino.org",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^6.0|^7.0|^8.0",
        "symfony/process": "^5.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0",
        "illuminate/view": "^6.0|^7.0|^8.0",
        "illuminate/auth": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tino\\Plugins\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tino\\Plugins\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tino\\Plugins\\PluginsServiceProvider"
            ]
        }
    }
}
